<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.esiea.vergues_andriamaro.legrandprojet">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.Gen4Activity" />
        <activity android:name=".activities.MH4UActivity" />
        <activity android:name=".activities.MHXXActivity" />
        <activity android:name=".activities.MH3UActivity" />
        <activity android:name=".activities.MHFUActivity" />
        <activity android:name=".activities.MHActivity"></activity>
    </application>

</manifest>